# 阿里云凭证
ALIYUN_ACCESS_KEY_ID=your_ak
ALIYUN_ACCESS_KEY_SECRET=your_sk
ALIYUN_REGION_ID=cn-hangzhou

# 主域名 (不含 RR)
ALIYUN_DOMAIN=example.com

# TTL (秒), 默认 600
ALIYUN_TTL=600

# 待更新记录 (JSON 数组格式)
# 每项: rr = 主机记录, ll = 对应的 link-local IPv6 (fe80::…)
DDNS_RECORDS=[{"rr":"www","ll":"fe80::266e:96ff:fed0:bc74"},{"rr":"blog","ll":"fe80::aabb:ccdd:eeff:1122"}]

# 服务端口
API_PORT=3000

# 访问令牌 (用于 API 安全认证, 建议设置为复杂的随机字符串)
API_TOKEN=your_secure_token
